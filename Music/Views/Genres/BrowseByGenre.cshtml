@model IEnumerable<Music.Models.Album>

@{
    ViewBag.Title = "GenreSort";
}

<h2>@ViewBag.header Albums:</h2>
<link href="~/Content/StyleSheet1.css" rel="stylesheet" />
<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                <a href="/Albums/Details/@item.AlbumID">@Html.DisplayFor(modelItem => item.Title)</a>
            </td>

        </tr>
    }

</table>
<br />
<br />
<a href="/Genres/Index" class="btn btn-sm btn-default">Back</a>
